<input type="hidden" name = "pw" value="${member.pw}">
<input type="hidden" name = "m_name" value="${member.m_name}">
<input type="hidden" name = "m_gender" value="${member.m_gender}">
<input type="hidden" name = "m_ssno" value="${member.m_ssno}">
<input type="hidden" name = "m_pno" value="${member.m_pno}">
<input type="hidden" name = "m_address" value="${member.m_address}">
<input type="hidden" name = "m_agree" value="${member.m_agree}">





</body>
