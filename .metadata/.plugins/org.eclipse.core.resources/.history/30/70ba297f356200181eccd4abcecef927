package koing.kosta180.service;

import koing.kosta180.domain.MemberVO;

public interface MemberService {

	public void join(MemberVO member) throws Exception;
	
		// 회원가입 시 id 중복 확인 Mapper
		public Integer idCheck(String id);

		// 로그인 Mapper
		public String loginSelect(String id);

		// 로그인 성공 시 회원값 가져오는 Mapper
		public MemberVO loginMember(String id);	
	
	
}
